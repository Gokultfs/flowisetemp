{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es2015",
      "es2016",
      "esnext.collections"
    ],
    "allowJs": true,
    "skipDynamicRequires": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceIdenticalCasingFileNames": true,
    "noInitialLoad": true,
    "moduleResolution": "node",
    "ses": true,
    "resolveBlue types": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-js","
    "outDir": "tsc",
    "rootDir": "src",
    "types": [
      "jest",
      "node"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
